{
  "name": "prisma-fastify-template",
  "version": "1.0.1",
  "description": "A simple Prisma + Fastify template.",
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "server.ts",
  "scripts": {
    "dev": "tsx watch server.ts | pino-pretty",
    "build": "prisma generate && tsc",
    "start": "node dist/server.js",
